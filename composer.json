{
    "name": "tenants/flashcards",
    "description": "Flashcards tenant for Larabis multi-tenant application",
    "type": "library",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "App\\Features\\Auth\\": "app/Features/Auth/",
            "App\\Features\\Flashcards\\": "app/Features/Flashcards/"
        },
        "classmap": [
            "database/seeders"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
